{
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	//"plugins": ["@typescript-eslint"],
	"parser": "@typescript-eslint/parser",
    "extends": [
		"eslint:recommended",
		"prettier"
	],
    //"plugins": ["prettier"],
	"parserOptions": {
		"project": "tsconfig.json",
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
    "rules": {
        //"prettier/prettier": "error",
		"camelcase": ["error"],
		"eqeqeq": ["error", "always"]
    }
}