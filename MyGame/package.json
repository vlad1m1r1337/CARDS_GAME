{
    "name": "vite-test-ts",
    "private": true,
    "version": "0.0.0",
    "type": "commonjs",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "format": "prettier --write ./src",
        "lint:ts": "eslint ./src/*.ts",
        "lint:css": "stylelint ./src/index.css",
