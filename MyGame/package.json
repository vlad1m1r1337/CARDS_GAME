    },
    "dependencies": {
        "@types/node": "^20.3.1",
        "assert": "^2.0.0",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.3",
        "jest": "^29.6.1",
        "prettier": "^2.8.8",
        "stylelint": "^15.6.3",
        "stylelint-config-prettier": "^9.0.5",
        "stylelint-config-standard": "^33.0.0",
        "stylelint-prettier": "^3.0.0",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.2",
        "vite": "^4.3.2"
    }
